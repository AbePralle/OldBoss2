module Boss
  uses ParseKit<<Boss>>

class BossScope
  PROPERTIES
    this_module : BossModule
    this_type   : BossType
    this_method : BossMethod

  METHODS
    method init( this_module )

    method init( this_method )
      this_type = this_method.type_context
      this_module = this_type.module_context
endClass
